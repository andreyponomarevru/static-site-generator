{
  "name": "sssg",
  "version": "1.0.0",
  "description": "simple static site generator",
  "main": "index.js",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"nodemon --watch ./src --ext ts,css,json,md --exec 'TS_NODE_PROJECT=tsconfig.json node --inspect=0.0.0.0:9229 --require ts-node/register ./src/index.ts && npm run test'\" \"live-server --wait=500 ./build\"",
    "test": ""
  },
  "keywords": [
    "static",
    "html",
    "website",
    "generator",
    "nodejs",
    "markdown"
  ],
  "author": "Andrey Ponomarev",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/markdown-it": "^12.2.3",
    "@types/node": "^16.11.11",
    "concurrently": "^6.4.0",
    "fs-extra": "^10.0.0",
    "live-server": "^1.2.1",
    "markdown-it": "^12.2.0",
    "nodemon": "^2.0.15",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2"
  },
  "dependencies": {}
}
